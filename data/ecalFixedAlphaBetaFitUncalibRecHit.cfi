  # producer of rechits starting from digis
  module ecalFixedAlphaBetaFitUncalibRecHit =  EcalFixedAlphaBetaFitUncalibRecHitProducer{
   InputTag EBdigiCollection = ecalDigis:ebDigis
   InputTag EEdigiCollection = ecalDigis:eeDigis
   string EBhitCollection  = "EcalUncalibRecHitsEB"
   string EEhitCollection  = "EcalUncalibRecHitsEE"
   untracked double alpha  = 1.2
   untracked double beta   = 1.7
   untracked string AlphaBetaFilename  = "NOFILE"
  }
